{
  "name": "smartheit",
  "version": "0.0.1",
  "description": "Tools for JavaScript Scripting by smartheit",
  "keywords": [
    "openHAB",
    "smarthome"
  ],
  "license": "EPL-2.0",
  "author": "smartheit GmbH",
  "main": "src/index.js",
  "types": "types/smartheit.d.ts",
  "scripts": {
    "lint": "npx eslint src",
    "lint:fix": "npx eslint --fix src",
    "pretest": "npm run lint",
    "test": "jest --config jest.config.js --passWithNoTests",
    "types": "tsc --project build/tsconfig.json",
    "types:test": "tsc --project build/tsconfig.test.json",
    "webpack": "webpack -c build/webpack.config.js",
    "docs": "rm -Rf ./docs/* && jsdoc --configure build/jsdoc.conf.json",
    "prebuild": "npm run test",
    "build": "npm run webpack && npm run types && npm run types:test",
    "postbuild": "npm run docs"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.16.5",
    "docdash": "^2.0.1",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "jsdoc-tsimport-plugin": "^1.0.5",
    "openhab": "^5.9.0",
    "typescript": "^4.9.5",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "private": false
}
